   <events>
    <script name="Clicked" isEscaped="1"><![CDATA[main(int row, string column, string value)
{
  if(column != &quot;helpFile&quot; || this.cellFillRC(row, &quot;helpFile&quot;) == &quot;&quot;){
    return; // Other column clicked or component help not available, exit
  }
  string componentName = this.cellValueRC(row, &quot;componentName&quot;);
  if(this.cellValueRC(row, &quot;isSubcomponent&quot;)){
    componentName = substr(componentName, 1);
  }
  fwInstallation_showHelpFile(componentName);
}
]]></script>
    <script name="Initialize" isEscaped="1"><![CDATA[/** The table with installed components is filled with the installed components
*/
main()
{
  fwInstallation_ui_setTableAlternatingRowColors(TABLE_INSTALLED_COMPONENTS_SHAPE);
  fwInstallation_ui_displayInstalledComponents(TABLE_INSTALLED_COMPONENTS_SHAPE, LABEL_FILE_ISSUE_FEEDBACK_SHAPE,
                                               POLYGON_FILE_ISSUE_FEEDBACK_SHAPE, ShowInstalledSubComponents.state(0));
}
]]></script>
    <script name="DoubleClicked" isEscaped="1"><![CDATA[/**  if the user double clicks on the componentName column
the panel with the component details is shown
*/
main()
{
  int rowNum, colNum; // store the coordinates of current cell
  getValue(TABLE_INSTALLED_COMPONENTS_SHAPE, &quot;currentCell&quot;, rowNum, colNum);

  // get the name of a component
  string componentName = TABLE_INSTALLED_COMPONENTS_SHAPE.cellValueRC(rowNum, &quot;componentName&quot;);
  if(TABLE_INSTALLED_COMPONENTS_SHAPE.cellValueRC(rowNum, &quot;isSubcomponent&quot;)){
    componentName = substr(componentName, 1);
  }

  // get the column name
  string columnName = TABLE_INSTALLED_COMPONENTS_SHAPE.currentColumn();

  if((columnName == &quot;componentName&quot;) || (columnName == &quot;componentVersion&quot;)){
    const string TYPE_OF_DESCRIPTION = &quot;PVSSDB&quot;;
    // display the panel with component details
    ChildPanelOnCentral(&quot;fwInstallation/fwInstallationComponentDetails.pnl&quot;, &quot;Details of &quot; + componentName,
                        makeDynString(&quot;$componentName:&quot; + componentName, &quot;$descFile:&quot;,
                                      &quot;$typeOfDescription:&quot; + TYPE_OF_DESCRIPTION,
                                      &quot;$reduHostNum:&quot;+fwInstallationRedu_myReduHostNum()));
  }
}
]]></script>
    <script name="RightMousePressed" isEscaped="1"><![CDATA[main(int row, string column)
