class InfoFrame : ObjectGroup{
  string frameId;
  dyn_anytype infoElements;

  public InfoFrame(string _frameId, const dyn_anytype &amp;_infoElements){
    frameId = _frameId;
    infoElements = _infoElements;
  }

  public bool hasInfoElement(shared_ptr&lt;InfoElement&gt; infElem){
    return (dynContains(infoElements, infElem) &gt; 0);
  }

  void setVisibility(bool visible){
    setValue(frameId, &quot;visible&quot;, visible);
  }
};

const shared_ptr&lt;InfoFrame&gt; IF_CFG_MODIF = new InfoFrame(&quot;frmCfgModif&quot;, makeDynAnytype(IE_CFG_REMOVE, IE_CFG_ADD));
const shared_ptr&lt;InfoFrame&gt; IF_CLEANUP = new InfoFrame(&quot;frmCleanup&quot;, makeDynAnytype(IE_CLEANUP_FILES, IE_CLEANUP_DIRS_EMPT, IE_CLEANUP_DIRS_REC));

const dyn_anytype INFO_FRAMES = makeDynAnytype(IF_CFG_MODIF, IF_CLEANUP);
const int INFO_FRAMES_LEN = dynlen(INFO_FRAMES);

shared_ptr&lt;InfoFrame&gt; getInfoElementFrame(shared_ptr&lt;InfoElement&gt; infElem){
  for(int i=1;i&lt;=INFO_FRAMES_LEN;i++){
    if(INFO_FRAMES[i].hasInfoElement(infElem)){
      return INFO_FRAMES[i];
    }
  }
  return nullptr;
}

