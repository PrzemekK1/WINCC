  <script name="ScopeLib" isEscaped="1"><![CDATA[class ObjectGroup{
  setVisibility(bool visible){}

  public hide(){setVisibility(false);}
  public show(){setVisibility(true);}

  public static showOnlyGiven(shared_ptr&lt;ObjectGroup&gt; objectGroup, const dyn_anytype &amp;objectGroupsList){
    int objectGroupsListLen = dynlen(objectGroupsList);
    for(int i=1;i&lt;=objectGroupsListLen;i++){
      if(!equalPtr(objectGroup, objectGroupsList[i])){
        objectGroupsList[i].hide();
      }
    }
    objectGroup.show();
  }

  public static restoreAll(const dyn_anytype &amp;objectGroupsList){
    int objectGroupsListLen = dynlen(objectGroupsList);
    for(int i=1;i&lt;=objectGroupsListLen;i++){
      objectGroupsList[i].show();
    }
  }
};

