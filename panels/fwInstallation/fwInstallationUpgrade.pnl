class TableObj : ObjectGroup{
  shape tableShape;

  public TableObj(string tableId){
    tableShape = getShape(tableId);
    init();
  }

  init(){
    int colsNum = tableShape.columnCount();
    tableShape.columnStretchable(colsNum - 1, true);
    fwInstallation_ui_setTableAlternatingRowColors(tableShape);
  }

  setVisibility(bool visible){
    tableShape.visible = visible;
  }
};

fillTables(){
  dyn_string sectionCol, entryCol;
  int rows = obsoleteConfigLibsStringToCols(getDollarValue(&quot;$obsoleteCfgLibs&quot;), sectionCol, entryCol);
  tblCfgRemove.appendLines(rows, &quot;section&quot;, sectionCol, &quot;entry&quot;, entryCol);
  rows = getConfigEntriesToAddAsCols(sectionCol, entryCol);
  tblCfgAdd.appendLines(rows, &quot;section&quot;, sectionCol, &quot;entry&quot;, entryCol);

  dyn_string filesToRemove = strsplit(getDollarValue(&quot;$filesToRemove&quot;), &quot;|&quot;);
  tblCleanupFile.appendLines(dynlen(filesToRemove), &quot;path&quot;, filesToRemove);

  dyn_string dirsToRemove = strsplit(getDollarValue(&quot;$dirsToRemove&quot;), &quot;|&quot;);
  tblCleanupDirEmpt.appendLines(dynlen(dirsToRemove), &quot;path&quot;, dirsToRemove);

  dyn_string dirsToRmvRec = strsplit(getDollarValue(&quot;$dirsToRmvRec&quot;), &quot;|&quot;);
  tblCleanupDirRec.appendLines(dynlen(dirsToRmvRec), &quot;path&quot;, dirsToRmvRec);
}

int getConfigEntriesToAddAsCols(dyn_string &amp;sectionCol, dyn_string &amp;entryCol){
  sectionCol = makeDynString(&quot;ui&quot;, &quot;ctrl&quot;);
  entryCol = makeDynStringOfSize(2, &quot;LoadCtrlLibs = \&quot;fwInstallation/fwInstallation.ctl\&quot;&quot;);
  return 2;
}

int obsoleteConfigLibsStringToCols(const string &amp;cfgModifMapString, dyn_string &amp;sectionCol, dyn_string &amp;entryCol){
  dyn_string elements = strsplit(cfgModifMapString, &quot;#&quot;);
  int elementsLen = dynlen(elements, &quot;#&quot;);
  if(elementsLen % 2 != 0){
    DebugTN(&quot;Invalid mapping string: &quot; + cfgModifMapString, &quot;Cannot decode it&quot;);
    return 0;
  }

  for(int i=1;i&lt;=elementsLen;i+=2){
    dyn_string entries = strsplit(elements[i + 1], &quot;|&quot;);
    string section = elements[i];
    dyn_string sectionList = makeDynStringOfSize(dynlen(entries), section);
    dynAppend(sectionCol, sectionList);
    dynAppend(entryCol, entries);
  }
  int entryColLen = dynlen(entryCol);
  for(int i=1;i&lt;=entryColLen;i++){
    entryCol[i] = &quot;LoadCtrlLibs = \&quot;&quot; + entryCol[i] + &quot;\&quot;&quot;;
  }
  return entryColLen;
}

dyn_string makeDynStringOfSize(int size, string s = &quot;&quot;){
  dyn_string ds;
  ds[size] = s;
  for(int i=1;i&lt;=size-1;i++){
    ds[i] = s;
  }
  return ds;
}

