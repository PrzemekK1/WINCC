class InfoElement : ObjectGroup{
  shared_ptr&lt;TableObj&gt; tableObj;
  string labelId;
  string buttonMaxId;
  string spacerId;

  public InfoElement(string tableId, string _labelId, string _buttonMaxId, string _spacerId = &quot;&quot;){
    tableObj = new TableObj(tableId);
    labelId = _labelId;
    buttonMaxId = _buttonMaxId;
    spacerId = _spacerId;
  }

  public string getButtonMaxId(){return buttonMaxId;}

  void setVisibility(bool visible){
    visible?(tableObj.show()):(tableObj.hide());
    setValue(labelId, &quot;visible&quot;, visible);
    setValue(buttonMaxId, &quot;visible&quot;, visible);
    setValue(spacerId, &quot;visible&quot;, visible);
  }
};

const shared_ptr&lt;InfoElement&gt; IE_CFG_REMOVE = new InfoElement(&quot;tblCfgRemove&quot;, &quot;txtCfgRemoveLabel&quot;, &quot;btCfgRemoveMax&quot;, &quot;spcrCfgRemove&quot;);
const shared_ptr&lt;InfoElement&gt; IE_CFG_ADD = new InfoElement(&quot;tblCfgAdd&quot;, &quot;txtCfgAddLabel&quot;, &quot;btCfgAddMax&quot;, &quot;spcrCfgAdd&quot;);
const shared_ptr&lt;InfoElement&gt; IE_CLEANUP_FILES = new InfoElement(&quot;tblCleanupFile&quot;, &quot;txtCleanupFileLabel&quot;, &quot;btCleanupFileMax&quot;, &quot;spcrCleanupFile&quot;);
const shared_ptr&lt;InfoElement&gt; IE_CLEANUP_DIRS_EMPT = new InfoElement(&quot;tblCleanupDirEmpt&quot;, &quot;txtCleanupDirEmptLabel&quot;, &quot;btCleanupDirEmptMax&quot;, &quot;spcrCleanupDirEmpt&quot;);
const shared_ptr&lt;InfoElement&gt; IE_CLEANUP_DIRS_REC = new InfoElement(&quot;tblCleanupDirRec&quot;, &quot;txtCleanupDirRecLabel&quot;, &quot;btCleanupDirRecMax&quot;, &quot;spcrCleanupDirRec&quot;);

const dyn_anytype INFO_ELEMENTS = makeDynAnytype(
    IE_CFG_REMOVE, IE_CFG_ADD, IE_CLEANUP_FILES, IE_CLEANUP_DIRS_EMPT, IE_CLEANUP_DIRS_REC);
const int INFO_ELEMENTS_LEN = dynlen(INFO_ELEMENTS);

shared_ptr&lt;InfoElement&gt; getInfoElementFromButtonMaxId(string buttonMaxId){
  string thisButtonMaxId = this.name();
  for(int i=1;i&lt;=INFO_ELEMENTS_LEN;i++){
    if(INFO_ELEMENTS[i].getButtonMaxId() == thisButtonMaxId){
      return INFO_ELEMENTS[i];
    }
  }
  return nullptr;
}

