showRightClickPopupMenu(int row, dyn_string columns){
  if(row &lt; 0){return;}

  const int menuIdCopyComponents = 1;
  dyn_string menu = makeDynString(&quot;PUSH_BUTTON, Copy table to clipboard, &quot; + menuIdCopyComponents + &quot;, 1&quot;);

  int selectedItemId;
  popupMenu(menu, selectedItemId);

  switch(selectedItemId)
  {
    case menuIdCopyComponents:
      setClipboardText(fwInstallation_ui_getTableColumnsAsString(getShape(this.name()), columns, false));
      break;
  }
}
