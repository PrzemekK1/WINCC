const string BT_MAXIMIZE_LABEL_MAXIMIZE = &quot;Maximize&quot;;
const string BT_MAXIMIZE_LABEL_RESTORE = &quot;Restore All&quot;;

maximizeBtnClicked(){
  string thisButtonId = this.name();
  string currentButtonLabel = this.text();
  string newButtonLabel;
  if(currentButtonLabel == BT_MAXIMIZE_LABEL_MAXIMIZE){
    newButtonLabel = BT_MAXIMIZE_LABEL_RESTORE;
    shared_ptr&lt;InfoElement&gt; infoElement = getInfoElementFromButtonMaxId(thisButtonId);
    shared_ptr&lt;InfoFrame&gt; infoFrame = getInfoElementFrame(infoElement);
    InfoElement::showOnlyGiven(infoElement, INFO_ELEMENTS);
    InfoFrame::showOnlyGiven(infoFrame, INFO_FRAMES);
  }else{
    newButtonLabel = BT_MAXIMIZE_LABEL_MAXIMIZE;
    InfoElement::restoreAll(INFO_ELEMENTS);
    InfoFrame::restoreAll(INFO_FRAMES);
  }
  this.text = newButtonLabel;
}

